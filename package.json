{
  "name": "Homesourcing-Api",
  "version": "0.1.0",
  "description": "Homesourcing Social Media Dashboard Management",
  "main": "index.js",
  "scripts": {
    "lint": "eslint index.js ; exit 0",
    "restart": "psql -c 'DROP DATABASE postpg' && createdb postpg & npm run start",
    "start": "cross-env NODE_ENV=development nodemon index.js",
    "run:dev": "cross-env NODE_ENV=development nodemon build/index.js",
    "test": "cross-env NODE_ENV=test && ./node_modules/.bin/mocha -R spec 'test/**/**/**/**test.js' ",
    "eslint": "eslint ./",
    "eslint-fix": "eslint ./ --fix",
    "swagger-autogen": "node swagger.js",
    "build": "cross-env NODE_ENV=production babel ./ --ignore node_modules,@Docs --out-dir dist"
  },
  "keywords": [
    "Node.js",
    "Express",
    "netlify",
    "express.js"
  ],
  "author": "Homesourcing",
  "license": "ISC",
  "dependencies": {
    "atob": "^2.1.2",
    "axios": "^1.1.3",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.2",
    "express": "^4.18.2",
    "express-jwt": "^7.7.5",
    "fbgraph": "^1.4.4",
    "handlebars": "^4.7.7",
    "handlebars-helpers": "^0.10.0",
    "helmet": "^6.0.0",
    "jsonwebtoken": "^8.5.1",
    "mailgun-js": "^0.22.0",
    "method-override": "^3.0.0",
    "mocha": "^10.0.0",
    "morgan": "^1.10.0",
    "node-gyp": "^9.3.1",
    "nodemon": "^2.0.19",
    "npm": "^9.5.0",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.21.6",
    "swagger-autogen": "^2.23.1",
    "swagger-ui-express": "^4.6.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/node": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "eslint": "^8.23.0"
  }
}
